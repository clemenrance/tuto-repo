pipeline{

    agent any

    stages{

        stage('Gitcheckout'){  

            steps{
                git branch: 'main', url: 'https://github.com/clemenrance/tuto-repo.git'
            }
        }
        stage('untit test'){

            steps{
                sh 'mvn test'
            }
        }
        stage('Integration Test'){

            steps{
                sh 'mvn verify -DskipUnitTests'
            }
        }
        stage('Artifact Build'){

            steps{
                sh 'mvn clean install'
            }
        }
    }
}